from sklearn.ensemble import RandomForestRegressor
from sklearn.datasets import make_regression
import joblib

# Create dummy data
X, y = make_regression(n_samples=1000, n_features=12, noise=0.1, random_state=42)

# Train model with tuned hyperparameters
model = RandomForestRegressor(
    n_estimators=149,
    max_depth=16,
    max_features='log2',
    min_samples_split=5,
    min_samples_leaf=1,
    random_state=42
)
model.fit(X, y)

# Save the model
joblib.dump(model, "rf_model.pkl")
